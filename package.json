{
  "name": "exhibit-plugin-sass",
  "version": "0.1.2",
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-runtime"
    ]
  },
  "dependencies": {
    "babel-runtime": "^6.6.1",
    "bluebird": "^3.3.4",
    "convert-source-map": "^1.2.0",
    "lodash": "^4.16.4",
    "node-sass": "^3.4.2",
    "sander": "^0.4.0",
    "subdir": "0.0.3"
  },
  "devDependencies": {
    "babel-cli": "^6.6.5",
    "babel-eslint": "^6.0.2",
    "babel-plugin-transform-runtime": "^6.6.0",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-0": "^6.5.0",
    "del-cli": "^0.2.0",
    "eslint": "^2.6.0",
    "eslint-config-airbnb": "^6.2.0",
    "eslint-plugin-babel": "^3.1.0",
    "exhibit": "^0.8.3",
    "test-exhibit-plugin": "^0.1.2",
    "exhibit": "^0.8.3"
  },
  "engines": {
    "node": ">=0.12"
  },
  "eslintConfig": {
    "extends": "airbnb/base",
    "root": "true",
    "parser": "babel-eslint",
    "rules": {
      "indent": [
        2,
        "tab"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "peerDependencies": {
    "exhibit": "^0.8.3"
  },
  "repository": "exhibitjs/exhibit-plugin-sass",
  "scripts": {
    "build": "del dist && babel src --out-dir dist --source-maps",
    "build:watch": "del dist && babel src --out-dir dist --watch --source-maps",
    "prepublish": "npm run build",
    "test": "eslint . && test-exhibit-plugin"
  }
}
